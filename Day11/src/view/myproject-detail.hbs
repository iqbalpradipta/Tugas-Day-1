<html>
  <head>
    <title>My Project</title>
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <link rel='stylesheet' href='/assets/css/myproject-detail.css' />
  </head>
  <body style='background-color: #e3e7eb'>
    <!-- Navbar -->
    <nav class='navbar'>
      <div class='div-navbar'>
        <ul style='display: flex; list-style: none; gap: 50px; align-items: center'>
          <li>
            <img src='/assets/image/brandred.png' alt='dumbwaysID' style='height: 50px' />
          </li>
          <li>
            <img src='/assets/icon/hamburger.png' title='hamburger icons' class='nav-hamburger' id='nav-hamburger' onclick='iconBar()' />
          </li>
          <li class='navbar-link nav-text'>
            <a href='index.html'> Home </a>
          </li>
          <li class='navbar-link nav-text'>
            <a href='myproject.html'> My Project </a>
          </li>
          <li class='navbar-link nav-text'>
            <a href='testimonial.html'> Testimonal </a>
          </li>
        </ul>
        <div>
          <button class='contact-nav'>
            <a href='contact.html'>Contact Me</a>
          </button>
        </div>
      </div>
    </nav>
    <div class='iconBar-container' id='iconBar-container'>
      <a href='index.html'>Home</a>
      <a href='myproject.html' class='active'>My Project</a>
      <a href='testimonial.html'>Testimonial</a>
      <a href='contact.html'>Contact Me</a>
    </div>

    <!-- End of Navbar -->

    <!-- main content -->
    <div class='main-content'>
      <h1 class='title'>{{data.title}} Page: {{data.id}}</h1>
      <div class='gambar'>
        <img src='/assets/image/gambarphone.png' class='img' />
      </div>
      <!-- detail -->
      <div class='detail'>
        <h3 class='duration'>Duration</h3>
        <p class='date'>12 Jan 2021 - 11 Feb 2021</p>
        <img src='/assets/icon/calendar.png' class='calender' />
        <p class='month'>1 Month</p>
        <img src='/assets/icon/clock.png' class='times' />
      </div>
      <!-- end of detail -->
      <!-- tech -->
      <div class='tech'>
        <h3 class='duration'>Technologies</h3>
        <p class='date'>React JS</p>
        <img src='/assets/icon/calendar.png' class='calender' />
        <p class='month'>Node JS</p>
        <img src='/assets/icon/clock.png' class='times' />
        <p class='js'>Javascript</p>
        <img src='/assets/icon/icons8-javascript-50.png' class='jslogo' />
        <p class='socket'>Socket IO</p>
        <img src='/assets/icon/socket_io_logo_icon_144874.png' class='socketlogo' />
      </div>
    </div>
    <!-- end of tech -->

    <!-- main content -->
    <p class='content'>
      {{data.description}}
    </p>
    <!-- end of main content -->

    <!-- End Main Content -->
    <!-- End of Outer -->
    <script src='/assets/js/nav.js'></script>
  </body>
</html>